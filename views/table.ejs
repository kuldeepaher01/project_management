        <table class="table table-striped custom-table">
          <thead>
            <tr>
              <th scope="col">PRN No</th>
              <th scope="col">Name</th>
              <th scope="col">Course Name</th>
              <th scope="col">Contact</th>
              <th scope="col">Course Guid Name</th>
              <th scope="col">Project Title</th>
              <th scope="col">Div</th>
              <th scope="col">Batch</th>
              <th scope="col">Group</th>
              <th scope="col">Email</th>
              <th scope="col">Tech Used</th>
              <th scope="col">Project Desc</th>
            </tr>
          </thead>
          <tbody>
            <% result.forEach(function(post){ %>
              <tr scope="row">
                  <td><%=post.PRN_NO%></td>
                  <td><a href="#"><%=post.studentName%></a></td>
                  <td><%=post.courseName%></td>
                  <td><%=post.phoneNo%></td>
                  <td><%=post.projectGuidName%></td>
                  <td><%=post.projectTitle%></td>
                  <td><%=post.division%></td>
                  <td><%=post.batch%></td>
                  <td><%=post.groupNo%></td>
                  <td><%=post.email%></td>
                  <td><%=post.techUsed%></td>
                  <td><%=post.projectDesc%></td>
                 
                  <td><a href="#" class="more">Details</a></td>
                </tr>
             <% }) %>
